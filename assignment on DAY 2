{
  "rec":{
       "chr":"<%=rec.chrom%>",
        "pos":<%=rec.pos%>,
        "ref":"<%=rec.ref%>",
        "alt":"<%=rec.alt[0]%>",
        "dp":"<%=rec.info.dp%>",
        "samples":[
        <%=
        a=[]
        rec.each_sample {|s|a.push s.dp}
        a.join(',')
        %>
]
}
}

